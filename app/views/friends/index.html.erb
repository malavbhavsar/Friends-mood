<table class="table table-hover">
      <tr>
        <th>Name</th>
        <th>Status</th>
        <th>Mood</th>
      </tr>
      <% @friends.each do |f| %>
      <% if (f["status"]["sentiment"]==":)") %>
      <tr class= "success"><% elsif (f["status"]["sentiment"]==":(") %>
       <tr class="error"   ><% else  %>
      <tr class="warning">
      <% end %>
        <td><%= link_to f["name"], 'http://www.facebook.com/'+f["id"] %></td>
        <td><%= f["status"]["text"] %></td>
        <td><%= f["status"]["sentiment"] %></td>
      </tr>
      <%end %>
</table>

